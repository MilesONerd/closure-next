{
  "name": "@closure-next/core",
  "version": "1.0.9",
  "description": "Modern TypeScript-based reimplementation of Google Closure Library with framework integrations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --build tsconfig.build.json",
    "test:compile": "tsc --build tsconfig.test.json",
    "test": "npm run test:compile && jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Closure",
    "TypeScript",
    "JavaScript",
    "Closure Library",
    "MilesONerd",
    "Closure Next"
  ],
  "author": "Closure Next Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MilesONerd/closure-next.git"
  },
  "devDependencies": {
    "@angular/compiler": "^19.1.2",
    "@angular/compiler-cli": "^19.1.2",
    "@angular/core": "^19.1.2",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@sveltejs/kit": "^2.16.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^29.5.14",
    "@types/jquery": "^3.5.32",
    "@types/jsdom": "^21.1.7",
    "@types/next": "^8.0.7",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.7.0",
    "cypress": "^14.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.0.0",
    "svelte": "^5.19.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@angular/core": "^17.0.0",
    "react": "^18.0.0",
    "svelte": "^4.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  }
}
