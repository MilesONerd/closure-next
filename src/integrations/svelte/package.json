{
  "name": "@closure-next/svelte",
  "version": "1.0.3",
  "description": "Svelte integration for Closure Next",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "pretest": "tsc --noEmit",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --config=jest.config.mjs --detectOpenHandles --runInBand",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.mjs --watch"
  },
  "peerDependencies": {
    "@closure-next/core": "^1.0.10"
  },
  "devDependencies": {
    "@asamuzakjp/css-color": "^1.1.2",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@happy-dom/jest-environment": "^17.0.2",
    "@jest/globals": "^29.7.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^4.2.3",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "babel-jest": "^29.7.0",
    "cssstyle": "^3.0.0",
    "happy-dom": "^17.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "lru-cache": "^11.0.2",
    "svelte": "^4.2.19",
    "svelte-jester": "^3.0.0",
    "svelte-preprocess": "^6.0.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "Closure",
    "TypeScript",
    "JavaScript",
    "Closure Library",
    "MilesONerd",
    "Closure Next",
    "Svelte"
  ],
  "author": "Closure Next Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MilesONerd/closure-next.git"
  }
}
