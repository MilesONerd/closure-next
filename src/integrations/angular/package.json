{
  "name": "@closure-next/angular",
  "version": "1.0.6",
  "description": "Angular integration for Closure Next",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Angular",
    "Closure",
    "TypeScript",
    "SSR"
  ],
  "author": "Closure Next Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MilesONerd/closure-next.git"
  },
  "peerDependencies": {
    "@angular/common": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@closure-next/core": "^1.0.0"
  },
  "devDependencies": {
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.7.3"
  }
}
